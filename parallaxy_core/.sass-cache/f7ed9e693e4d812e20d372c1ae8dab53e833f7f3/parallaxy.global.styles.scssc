    text-decoration: none;

    &:link,
    &:visited {
      background: #444;
      background: rgba(0, 0, 0, 0.6);
      color: $page;
      display: block;
      line-height: 2;
      padding: 0;
      text-align: center;
      text-decoration: none;
    }
    &:hover,
    &:focus,
    &:active {
      outline: 0;
    }
  }
}



/* =============================================================================
 *   Tabs (local tasks)
 * ========================================================================== */
#tasks {
  padding: 10px 10px 10px 50px;
  background: $tasks_background_color;
  @include box-shadow(0px 3px 4px rgba(0,0,0,.1) inset, 0px -1px 0px lighten($tasks_background_color, 5%));
  border-bottom: 1px solid darken($tasks_background_color, 5%);
  position: relative;
  // margin: 0 $panel-gutter-width 1px;
  margin-bottom: 10px;

  &:before {
    content: "";
    @include sprites-sprite(config, $dimensions: true);
    position: absolute;
    top: 10px;
    left: 10px;
    display: block;
  }

  ul.primary {
    margin: 0;
    border-bottom: none;
  }
}


ul.primary {
  border-bottom-color:#ccc;
  margin: 20px 0;
  padding: 0 0 0 5px;

  li {
    display: block;
    float: left;
    margin: 0 1px -1px;

    a {
      background-color: lighten($tasks_background_color, 5%);
      border-color: darken($tasks_background_color, 5%);
      margin-right: 10px;
      padding: 5px 10px;
      display: block;
      float: left;
      line-height: normal;
      @include border-radius(20px);
      @include box-shadow(0px 1px 0px lighten($tasks_background_color, 10%), 0px -3px 3px rgba(0,0,0,.1) inset);
      color: #ccc;

      &:hover,
      &:focus {
        background-color: $link_color;
        border: 1px solid darken($link_color, 10%);
        color: #fff;
      }
    }
  }

  li.active a,
  li.active a:hover,
  li.active a:focus {
    background-color: $link_color;
    border: 1px solid darken($link_color, 10%);
    color: #fff;
  }
}


ul.secondary {
  border-bottom: 1px solid #ccc;
  margin: 1em 0 0;
  padding: 0 .3em 1em;

  li {
    border-right: 0;
    list-style: none;
    padding: 0 10px 0 0;

    a {
      &:hover,
      &.active {
        border-bottom: none;
        text-decoration: underline;
      }
    }
  }
}


