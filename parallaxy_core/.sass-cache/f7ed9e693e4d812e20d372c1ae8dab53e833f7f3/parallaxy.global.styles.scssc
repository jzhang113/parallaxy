/* =============================================================================
 *   Primary, Secondary and Menu Bar region menus
 * ========================================================================== */

/*
 * Use one of the following id's for granular control:
 *  - #menu-bar (menu bar region)
 *  - #primary-menu (main menu)
 *  - #secondary-menu (secondary/user menu)
 * You can target all of these with .nav or div[id$="menu-bar"]
 */

.nav {
  clear: both;

  ul,
  ul.menu {
    margin: 0;
    padding: 0;
  }

  li,
  ul.menu li {
    display: inline;
    float: left;
    list-style: none;
    margin: 0;
    padding: 0;
  }

  li a,
  ul.menu li a {
    display: block;
    white-space: nowrap;

    &:visited {}

    &:hover,
    &:focus {}
  }

  .block {
    margin-bottom: 0;
  }
}

.nav ul.menu {
  border-left: 1px solid #000;
  @include box-shadow(1px 0px 0px rgba(255,255,255,.2) inset);

  li {
    width: 25%;
    border-right: 1px solid #000;
    @include box-shadow(1px 0px 0px rgba(255,255,255,.2));
  }

  a {
    text-align: center;
    padding: 12px 5px;
    font-weight: bold;
    @include font-size(1.9);
    color: $fye_gold;
    @include text-shadow(0px -1px 0px rgba(0,0,0,.5));
    letter-spacing: -1px;


    &:hover, &:focus, &:active {
      color: #f5f5f5;
      text-decoration: none;
      @include text-shadow(0px 1px 0px rgba(0,0,0,.5));
    }
  }

  #oniowa {
    @include ir;
    position: relative;
    min-height: 52px;
    padding: 0;

    &:before {
      content: "";
      position: absolute;
      top: 8px;
      left: 50%;
      @include sprites-sprite(oniowa-logo-gold, $dimensions:true);
      margin-left: -63px;
    }
  }
  #oniowa:hover:before, #oniowa:focus:before, #oniowa:active:before {
    @include sprites-sprite(oniowa-logo-white, $dimensions:true);
  }

  .active-trail,
  .active {
    background-color: $fye_gold;
    color: #111;
    @include text-shadow(0px 1px 0px rgba(255,255,255,.6));
    @include box-shadow(0px 0px 20px rgba(0,0,0,.4) inset);

    &:hover, &:focus, &:active {
      background-color: rgba($fye_gold, .3);
    }
  }

}


/* =============================================================================
